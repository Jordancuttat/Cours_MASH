    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }
tei_sex =
  
  ## (sexe) précise le sexe d'une personne. [13.3.2.1. Personal Characteristics]
  element sex {
    tei_macro.phraseSeq,
    tei_att.global.attributes,
